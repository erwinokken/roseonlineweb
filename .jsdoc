{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./client/", "./README.md"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "../../node_modules/jaguarjs-jsdoc",
    "destination": "./docs/",
    "private": true,
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "applicationName": "ROSEBrowser",
    "linenums": true
  }
}
